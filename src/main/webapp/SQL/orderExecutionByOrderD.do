SQL=SELECT E.PATIENT_ID AS brid,\r\n       E.VISIT_ID AS zyid,\r\n       E.ORDER_NO AS yzoid,\r\n       (O.ORDER_TEXT) AS yz_item,\r\n       'D' AS yz_type,\r\n       E.administration AS ORDER_CLASS_NAME,\r\n       O.REPEAT_INDICATOR AS yz_chlin,\r\n       E.IS_EXECUTE AS ISEXECUTE,\r\n       O.DOSAGE AS yz_jinum,\r\n       O.DOSAGE_UNITS AS yz_danwei,\r\n       to_char(e.SCHEDULE_PERFORM_TIME, 'yyyy-mm-dd hh24\:mi\:ss') AS zx_time\r\n  FROM ORDERS_EXECUTE E, ORDERS O\r\n WHERE E.PATIENT_ID \= '%s'\r\n   AND E.VISIT_ID \= %s\r\n   AND e.order_class \= 'E'\r\n   AND E.PATIENT_ID \= O.PATIENT_ID\r\n   AND E.VISIT_ID \= O.VISIT_ID\r\n   AND E.ORDER_NO \= O.ORDER_NO\r\n   AND E.ORDER_SUB_NO \= O.ORDER_SUB_NO\r\n   AND to_char(E.SCHEDULE_PERFORM_TIME, 'YY-MM-DD') \=\r\n       to_char(SYSDATE, 'YY-MM-DD')\r\n   AND (E.SCHEDULE_PERFORM_TIME > trunc(SYSDATE) AND\r\n       E.SCHEDULE_PERFORM_TIME < trunc(SYSDATE + 1))\r\n   AND ((E.REPEAT_INDICATOR IS NULL OR E.REPEAT_INDICATOR \= '0') OR\r\n       (E.REPEAT_INDICATOR \= '1' AND\r\n       (O.STOP_DATE_TIME IS NULL OR\r\n       O.STOP_DATE_TIME >\= E.SCHEDULE_PERFORM_TIME)))\r\n ORDER BY zx_time ASC, yzoid asc
