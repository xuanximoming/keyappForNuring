SQL=SELECT ORDERS.PATIENT_ID AS brid,\r\n       orders.VISIT_ID AS zyid,\r\n       orders.ORDER_TEXT AS yz_item,\r\n       orders.ORDER_CLASS AS yz_type,\r\n       (SELECT CLASS_NAME\r\n          FROM CLINIC_ITEM_CLASS_DICT\r\n         WHERE CLASS_CODE \= ORDERS.ORDER_CLASS) ORDER_CLASS_NAME,\r\n       orders.REPEAT_INDICATOR AS yz_chlin,\r\n       decode(orders.REPEAT_INDICATOR,\r\n              0,\r\n              '\#000000',\r\n              1,\r\n              '\#ff6600',\r\n              '\#000000') AS YZ_COLOR,\r\n       orders.DOSAGE AS yz_jinum,\r\n       (CASE\r\n         WHEN orders.DOSAGE_UNITS IS NULL THEN\r\n          ' '\r\n         ELSE\r\n          orders.DOSAGE_UNITS\r\n       END) AS yz_danwei,\r\n       orders.START_DATE_TIME AS zx_time\r\n  FROM ORDERS\r\n WHERE PATIENT_ID \= '%s'\r\n   AND VISIT_ID \= %s\r\n
